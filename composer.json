{
  "name": "techghor/laravel-license-manager",
  "description": "A Laravel package for managing software licenses",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "TechGhor",
      "email": "your.email@example.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "TechGhor\\LaravelLicenseManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TechGhor\\LaravelLicenseManager\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["TechGhor\\LaravelLicenseManager\\LicenseManagerServiceProvider"]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

